{
  "name": "eslint-config-zen",
  "version": "2.0.4",
  "description": "Provided as an extensible shared config",
  "main": "index.js",
  "scripts": {
    "bump:patch": "npm version patch -m \"v%s\"",
    "bump:minor": "npm version minor -m \"v%s\"",
    "bump:major": "npm version major -m \"v%s\"",
    "bump": "npm run bump:patch",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iansinnott/eslint-config-zen.git"
  },
  "author": "Ian Sinnott <ian@iansinnott.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iansinnott/eslint-config-zen/issues"
  },
  "homepage": "https://github.com/iansinnott/eslint-config-zen",
  "keywords": [
    "eslint",
    "eslintconfig",
    "babel",
    "react",
    "flowtype"
  ],
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.10.2",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-react": "^6.7.1"
  },
  "peerDependencies": {
    "eslint": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "lodash.mergewith": "^4.6.0"
  }
}
